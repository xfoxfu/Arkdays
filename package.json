{
  "name": "arkdays",
  "version": "1.5.0",
  "description": "custom server for arknights",
  "main": "dist/index.js",
  "private": true,
  "scripts": {
    "build": "tsc",
    "clean": "rm -rf dist",
    "format": "prettier --write 'src/**/*.ts'",
    "lint": "eslint 'src/*'",
    "start:debug": "tsnd --inspect=5858 src/main",
    "start:dev": "tsnd src/main",
    "start:prod": "node dist/main.js",
    "start": "ts-node src/main",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xfoxfu/Arkdays.git"
  },
  "author": "Arkdays <admin@arkdays.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/xfoxfu/Arkdays/issues"
  },
  "homepage": "https://github.com/xfoxfu/Arkdays#readme",
  "devDependencies": {
    "@types/express": "^4.17.12",
    "@types/level": "^6.0.0",
    "@types/lodash": "^4.14.170",
    "@types/node": "^15.6.1",
    "@typescript-eslint/eslint-plugin": "^4.26.0",
    "@typescript-eslint/parser": "^4.26.0",
    "eslint": "^7.27.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^3.4.0",
    "got": "^11.8.2",
    "husky": "^6.0.0",
    "lint-staged": "^11.0.0",
    "prettier": "^2.3.0",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^1.1.6",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.17",
    "@nestjs/config": "^0.6.3",
    "@nestjs/core": "^7.6.17",
    "@nestjs/platform-express": "^7.6.17",
    "date-fns": "^2.22.1",
    "jose": "^3.12.2",
    "level": "^7.0.0",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^6.0.0"
  },
  "lint-staged": {
    "src/*.{js,json,css,md,ts}": [
      "prettier --write",
      "eslint"
    ]
  },
  "prettier": {
    "trailingComma": "all"
  }
}
